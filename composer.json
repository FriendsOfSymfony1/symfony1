{
    "name": "james-and-james-fulfilment/symfony1",
    "type": "library",
    "description": "Fork of symfony 1.4 with dic, form enhancements, latest swiftmailer and better performance",
    "keywords": ["framework", "symfony1", "doctrine1"],
    "license": "MIT",
    "require": {
        "php" : "^7.4 || ^8.1",
        "friendsofsymfony1/swiftmailer": "^6.2.5"
    },
    "require-dev": {
        "psr/log": "*"
    },
    "support": {
        "issues": "https://github.com/JamesAndJamesFulfilment/symfony1/issues",
        "source": "https://github.com/JamesAndJamesFulfilment/symfony1"
    },
    "autoload": {
        "files": ["autoload.php"]
    },
    "suggest": {
        "james-and-james-fulfilment/doctrine1": "Doctrine plugin",
        "psr/log": "Psr logger"
    },
    "extra": {
        "branch-alias": {
            "dev-php82": "1.6-dev"
        }
    },
    "bin": ["data/bin/symfony"]
}
