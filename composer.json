{
    "name": "fruit/symfony1",
    "description": "Fork of Symfony v1.5",
    "version": "1.6.0-dev",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "swiftmailer/swiftmailer": "~5.2",
        "symfony/dependency-injection": "~3.1.1",
        "symfony/config": "~3.1.1",
        "symfony/yaml": "~3.1.1"
    },
    "require-dev": {
        "psr/log": "*"
    },
    "type": "library",
    "autoload": {
        "classmap": ["lib/"],
        "exclude-from-classmap": [
            "/lib/plugins/",
            "/lib/vendor/",
            "/lib/task/generator/skeleton/",
            "/lib/controller/default/",
            "/lib/helper/"
        ]
    },
    "suggest": {
        "lexpress/doctrine1": "Doctrine plugin",
        "propel/sf-propel-o-r-m-plugin": "Propel plugin",
        "psr/log": "Psr logger"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "symfony3-dic"
        }
    },
    "bin": ["data/bin/symfony"]
}
